<h1>Listing tutors</h1>

<table>
  <tr>
    <th>Experience</th>
    <th>Qualifications</th>
    <th>Course experience</th>
    <th>Textbook experience</th>
    <th>National knowledge</th>
    <th>Age</th>
    <th>Phone</th>
    <th>Sex</th>
    <th>Flavour</th>
    <th>Given name</th>
    <th>Family name</th>
    <th>Given name first</th>
  </tr>

<% @tutors.each do |tutor| %>
  <tr>
    <td><%=h tutor.experience %></td>
    <td><%=h tutor.qualifications %></td>
    <td><%=h tutor.course_experience %></td>
    <td><%=h tutor.textbook_experience %></td>
    <td><%=h tutor.national_knowledge %></td>
    <td><%=h tutor.age %></td>
    <td><%=h tutor.phone %></td>
    <td><%=h tutor.sex %></td>
    <td><%=h tutor.flavour %></td>
    <td><%=h tutor.given_name %></td>
    <td><%=h tutor.family_name %></td>
    <td><%=h tutor.given_name_first %></td>
    <td><%= link_to 'Show', tutor %></td>
    <td><%= link_to 'Edit', edit_tutor_path(tutor) %></td>
    <td><%= link_to 'Destroy', tutor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tutor', new_tutor_path %>